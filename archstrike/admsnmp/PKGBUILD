# Maintainer: ArchStrike <team@archstrike.org>

buildarch=212

pkgname=admsnmp
pkgver=0.1
pkgrel=4
groups=('archstrike' 'archstrike-scanners')
pkgdesc='ADM SNMP audit scanner'
url=('http://packetstorm.wowhacker.com/groups/ADM/')
license=('GPL')
depends=('glibc')
makedepends=('gcc')
arch=('i686' 'x86_64' 'armv6h' 'armv7h')
source=("http://packetstorm.wowhacker.com/groups/ADM/ADMsnmp.$pkgver.tgz")
sha512sums=('e861174206b87934ad14f686a5f0abc0d1350b94b535075b9d75d9c79411be6dd298cb2f496140541603025217d275fb80a6429f653bd3735e098b7cb6c0282e')

build() {
  cd ADMsnmp
  gcc $CFLAGS snmp.c -o admsnmp
}

package() {
  cd ADMsnmp
  install -Dm755 admsnmp "$pkgdir/usr/bin/$pkgname"
  install -dm755 "$pkgdir/usr/share/$pkgname"
  cp ADMsnmp.README snmp.passwd "$pkgdir/usr/share/$pkgname/"
}

