sha512sums=('0395d60dee2973d7514afa90c4017a7073af0821682c57bcc9f24d68c75c431d658cf0df2e3d8dbaff4aaf921420bb5766523f16d854d13a361d673fdb364bae')


package() {
  install -Dm755 $srcdir/hodor.py "${pkgdir}/usr/bin/hodor"
}
